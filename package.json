        "type-util": "^0.1.5",
        "typescript": "^2.5.2",
        "yup": "^0.23.0"
    },
    "devDependencies": {
        "@types/express": "^4.0.37",
        "@types/jest": "^19.2.3",
        "@types/node": "^7.0.18",
        "@types/jasmine": "^2.8.5",
