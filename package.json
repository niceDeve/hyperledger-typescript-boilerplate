        "@nestjs/core": "^4.0.0",
        "@nestjs/microservices": "^4.0.0",
        "@nestjs/swagger": "^1.1.4",
        "@nestjs/testing": "^4.0.0",
        "@nestjs/websockets": "^4.0.0",
        "@types/body-parser": "^1.16.8",
        "@types/node": "^8.5.8",
        "@types/object-hash": "^1.1.0",
